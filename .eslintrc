{
  "globals" : {
    "$"       : false,
    "_"       : false
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions"                   : true,
    "binaryLiterals"                   : true,
    "blockBindings"                    : true,
    "classes"                          : true,
    "defaultParams"                    : true,
    "destructuring"                    : true,
    "forOf"                            : true,
    "generators"                       : true,
    "globalReturn"                     : false,
    "jsx"                              : true,
    "modules"                          : true,
    "objectLiteralComputedProperties"  : true,
    "objectLiteralDuplicateProperties" : false,
    "objectLiteralShorthandMethods"    : true,
    "objectLiteralShorthandProperties" : true,
    "octalLiterals"                    : true,
    "regexUFlag"                       : true,
    "regexYFlag"                       : false,
    "restParams"                       : true,
    "spread"                           : true,
    "superInFunctions"                 : true,
    "templateStrings"                  : true,
    "unicodeCodePointEscapes"          : true,
  },
  "plugins": [
    "react"
  ],
  "env" : {
    "browser" : true,
    "node"    : true,
    "jquery"  : true,
    "es6"     : true,
    "amd"     : true
  },
  "rules": {
    "accessor-pairs"             : 0,
    "arrow-parens"               : 1,
    "block-scoped-var"           : 2,
    "camelcase"                  : [2, {"properties": "always"}],
    "comma-dangle"               : [2, "never"],
    "comma-spacing"              : 1,
    "complexity"                 : [1, 10],
    "constructor-super"          : 2,
    "curly"                      : [1, "multi-line"],
    "default-case"               : 1,
    "dot-notation"               : [1, {"allowKeywords": false}],
    "eqeqeq"                     : [2, "smart"],
    "guard-for-in"               : 1,
    "indent"                     : [1, 2],
    "max-depth"                  : [1, 3],
    "max-len"                    : [1, 120, 2],
    "max-nested-callbacks"       : [2, 3],
    "max-params"                 : [1, 3],
    "max-statements"             : [1, 20],
    "new-cap"                    : 1,
    "new-parens"                 : 1,
    "no-alert"                   : 0,
    "no-array-constructor"       : 2,
    "no-caller"                  : 0,
    "no-class-assign"            : 2,
    "no-cond-assign"             : 2,
    "no-const-assign"            : 2,
    "no-constant-condition"      : 2,
    "no-control-regex"           : 2,
    "no-debugger"                : 0,
    "no-delete-var"              : 2,
    "no-div-regex"               : 2,
    "no-dupe-args"               : 2,
    "no-dupe-keys"               : 2,
    "no-duplicate-case"          : 2,
    "no-else-return"             : 1,
    "no-empty"                   : 1,
    "no-empty-character-class"   : 2,
    "no-empty-label"             : 2,
    "no-eq-null"                 : 1,
    "no-eval"                    : 2,
    "no-ex-assign"               : 2,
    "no-extend-native"           : 2,
    "no-extra-bind"              : 1,
    "no-extra-boolean-cast"      : 2,
    "no-extra-semi"              : 1,
    "no-fallthrough"             : 2,
    "no-floating-decimal"        : 1,
    "no-func-assign"             : 2,
    "no-implied-eval"            : 2,
    "no-inner-declarations"      : 2,
    "no-invalid-regexp"          : 2,
    "no-irregular-whitespace"    : 1,
    "no-iterator"                : 2,
    "no-label-var"               : 1,
    "no-labels"                  : 0,
    "no-lone-blocks"             : 2,
    "no-lonely-if"               : 1,
    "no-loop-func"               : 2,
    "no-multi-spaces"            : 0,
    "no-multi-str"               : 2,
    "no-native-reassign"         : 2,
    "no-negated-in-lhs"          : 2,
    "no-nested-ternary"          : 2,
    "no-new"                     : 0,
    "no-new-func"                : 2,
    "no-new-object"              : 2,
    "no-new-wrappers"            : 2,
    "no-obj-calls"               : 1,
    "no-octal"                   : 1,
    "no-octal-escape"            : 2,
    "no-param-reassign"          : 1,
    "no-path-concat"             : 1,
    "no-process-exit"            : 2,
    "no-proto"                   : 2,
    "no-redeclare"               : 2,
    "no-regex-spaces"            : 1,
    "no-return-assign"           : 2,
    "no-script-url"              : 1,
    "no-self-compare"            : 2,
    "no-sequences"               : 2,
    "no-shadow"                  : 2,
    "no-shadow-restricted-names" : 2,
    "no-sparse-arrays"           : 2,
    "no-this-before-super"       : 2,
    "no-throw-literal"           : 2,
    "no-trailing-spaces"         : 0,
    "no-undef"                   : 2,
    "no-undef-init"              : 1,
    "no-underscore-dangle"       : 0,
    "no-unexpected-multiline"    : 2,
    "no-unneeded-ternary"        : 2,
    "no-unreachable"             : 2,
    "no-unused-expressions"      : 2,
    "no-unused-vars"             : 1,
    "no-use-before-define"       : 2,
    "no-useless-call"            : 2,
    "no-void"                    : 2,
    "no-with"                    : 2,
    "operator-linebreak"         : [2, "before"],
    "quotes"                     : [1, "single", "avoid-escape"],
    "radix"                      : 2,
    "require-yield"              : 2,
    "semi"                       : 2,
    "space-infix-ops"            : [1, {"int32Hint": false}],
    "use-isnan"                  : 2,
    "valid-typeof"               : 2,
    "wrap-iife"                  : [2, "inside"],
    "yoda"                       : [2, "never"],


    "react/display-name": 1,
    "react/jsx-boolean-value": 1,
    "react/jsx-curly-spacing": 1,
    "react/jsx-max-props-per-line": 1,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-quotes": 1,
    "react/jsx-sort-prop-types": 1,
    "react/jsx-sort-props": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-multi-comp": 1,
    "react/no-unknown-property": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/require-extension": 1,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "react/wrap-multilines": 1
  }
}
